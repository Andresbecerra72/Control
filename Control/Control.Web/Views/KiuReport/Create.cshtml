@model Control.Web.Models.KiuReportViewModel
@{
    ViewData["Title"] = "Index";
}

<h2>KIU FILE LOADER</h2>
<p>Load Excel (xlsx) file extension</p>
<section>
    <div align="center">
        <form asp-action="Create" method="post" enctype="multipart/form-data" autocomplete="off">

            <input asp-for="ExcelFile" type="file" class="form-control" accept=".xlsx" required>
            <br />
            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> SAVE</button>
            <a asp-action="Index" class="btn btn-success"><i class="fa fa-chevron-circle-down"></i> Back to List</a>
            <a asp-action="Delete" class="btn btn-danger"><i class="fa fa-trash"></i> Delete All Data</a>


        </form>
    </div>
</section>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}