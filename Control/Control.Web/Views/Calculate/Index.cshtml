@model Control.Web.Models.KiuReportViewModel


@{
    ViewData["Title"] = "Index";
}
    <asp:PlaceHolder ID="PlaceHolder1" runat="server" />
<h2>DATE Analysis</h2>
<hr />
<section>

    <div class="row">
        <div class="col-md-2">
            <form asp-action="Details" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="PublishOnKIU" class="control-label"></label>
                    <select asp-for="PublishOnKIU" asp-items="Model.Fechas" class="form-control"></select>
                    <span asp-validation-for="PublishOnKIU" class="text-danger"></span>
                </div>



                <div class="form-group">
                    <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> Create</button>

                </div>
            </form>
        </div>
    </div>

</section>

<section>

    <form id="form1" runat="server">
        <table style="width: 50%; text-align: center; background-color: skyblue;">
            <tr>
                <td align="center">
                    <asp:PlaceHolder ID="DBDataPlaceHolder" runat="server"></asp:PlaceHolder>
                </td>
            </tr>
        </table>
    </form>


</section>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}


}
